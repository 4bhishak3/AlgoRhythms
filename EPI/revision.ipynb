   ],
   "source": [
    "def swap_bits(n, i, j):\n",
    "    x, y = 1 << i, 1 << j\n",
    "    if not (\n",
    "        (n & x == x and n & y == y) or\n",
    "        (n & x == 0 and n & y == 0)\n",
    "    ): return n ^ (x | y)\n",
    "    return n\n",
    "swap_bits(14, 1, 5)"
   ]
  },
